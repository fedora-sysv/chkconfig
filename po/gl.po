#: ../chkconfig.c:23
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Isto pódese redistribuír libremente baixo os termos da Licencia Pública de "
"GNU.\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "uso:     %s --list [nome]\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nome>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nome>\n"

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <niveis>] <nome> %s)\n"

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "o servicio %s non soporta chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "erro lendo a información do servicio %s: %s\n"

#: ../chkconfig.c:100
msgid "on"
msgstr "activado"

#: ../chkconfig.c:100
msgid "off"
msgstr "desactivado"

#: ../chkconfig.c:117
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "erro ó abrir %s/init.d: %s\n"

#: ../chkconfig.c:151
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "erro lendo do directorio %s/init.d: %s"

#: ../chkconfig.c:231
#, c-format
msgid "%s version %s\n"
msgstr "%s versión %s\n"

#: ../chkconfig.c:238
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "só se pode indicar unha das opcións --list, --add, ou --del\n"

#: ../chkconfig.c:286
msgid "only one runlevel may be specfied for a chkconfig query\n"
msgstr ""
"só se pode indicar un nivel de execución para unha consulta de chkconfig\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Prema <F1> para máis información dun servicio."

#: ../ntsysv.c:56 ../ntsysv.c:93
msgid "Ok"
msgstr "Aceptar"

#: ../ntsysv.c:56
msgid "Back"
msgstr "Atrás"

#: ../ntsysv.c:56
msgid "Cancel"
msgstr "Cancelar"

#: ../ntsysv.c:68
msgid "What services should be automatically started?"
msgstr "¿Que servicios teñen que ser automáticamente iniciados?"

#: ../ntsysv.c:74
msgid "Services"
msgstr "Servicios"

#: ../ntsysv.c:164
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "erro lendo a información do servicio %s: %s\n"

#: ../ntsysv.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "erro lendo do directorio %s/init.d: %s\n"

#: ../ntsysv.c:219
msgid "bad argument to --levels\n"
msgstr "argumento erróneo de --levels\n"

#: ../ntsysv.c:226
msgid "No services may be managed by ntsysv!\n"
msgstr "¡Ningún servicio pode ser xestionado polo ntsysv!\n"

#: ../leveldb.c:265
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "imposible facer coincidi-lo patrón %s: %s\n"

#: ../leveldb.c:296
msgid "cannot determine current run level\n"
msgstr "non se pode determina-lo nivel de execución actual\n"

#: ../leveldb.c:330
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "imposible crea-la ligazón simbólica %s: %s\n"
