msgstr "Использование:   %s <enable|disable|is-enabled> [имя] \n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "формат:  %s [--list] [--type <type>] [имя]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <name>\n"

