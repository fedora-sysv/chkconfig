#: ../chkconfig.c:21
#, c-format
msgid "%s version %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"
msgstr "%s udgave %s - Copyright (C) 1997-2000 Red Hat, Inc.\n"

#: ../chkconfig.c:22
msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"Dette program må distribueres frit under vilkårene i GNU Public license.\n"

#: ../chkconfig.c:25
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr "brug:   %s --list [navn]\n"

#: ../chkconfig.c:26
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <navn>\n"

#: ../chkconfig.c:27
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <navn>\n"

#: ../chkconfig.c:28
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr "         %s [--level <levels>] <navn> %s)\n"

#: ../chkconfig.c:35
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"tjenesten %s understøtter chkconfig, men refereres ikke fra noget kørselsniveau (kør "
"'chkconfig --add %s')\n"

#: ../chkconfig.c:37
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "tjeneste %s understøtter ikke chkconfig\n"

#: ../chkconfig.c:39
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "fejl ved læsning af information om tjeneste %s: %s\n"

#: ../chkconfig.c:98 ../chkconfig.c:248
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "lykkedes ikke at åbne %s/init.d: %s\n"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "on"
msgstr "til"

#: ../chkconfig.c:211 ../chkconfig.c:216 ../chkconfig.c:313
msgid "off"
msgstr "fra"

#: ../chkconfig.c:282
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "fejl ved læsning fra mappe %s/init.d: %s"

#: ../chkconfig.c:290
msgid "xinetd based services:\n"
msgstr "xinetd baserede tjenester:\n"

#: ../chkconfig.c:292
#, c-format
msgid "failed to open directory %s: %s"
msgstr "lykkedes ikke at åbne %s: %s"

#: ../chkconfig.c:404
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:411
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "kun én af --list, --add eller --del må angives\n"

#: ../chkconfig.c:463
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "kun ét kørselsniveau må angives for en chkconfig forespørgsel\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "Tast <F1> for mere information om en tjeneste."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "O.k."

#: ../ntsysv.c:62
msgid "Back"
msgstr "Tilbage"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "Annullér"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "Hvilke tjenester skal startes automatisk?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "Tjenester"

#: ../ntsysv.c:186 ../ntsysv.c:230
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "fejl ved læsning af info for tjeneste %s: %s\n"

#: ../ntsysv.c:195
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "fejl ved læsning fra mappe %s/init.d: %s\n"

#: ../ntsysv.c:239
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "fejl ved læsning fra mappe %s: %s\n"

#: ../ntsysv.c:275
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Du skal være root for at køre %s.\n"

#: ../ntsysv.c:292
msgid "bad argument to --levels\n"
msgstr "dårligt argument til --levels\n"

#: ../ntsysv.c:299
msgid "No services may be managed by ntsysv!\n"
msgstr "Ingen tjenester må håndteres af ntsysv!\n"

#: ../leveldb.c:507
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "kunne 'globbe' mønster %s: %s\n"

#: ../leveldb.c:538
msgid "cannot determine current run level\n"
msgstr "kan ikke afgøre nuværende kørselsniveau\n"

#: ../leveldb.c:626
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "kunne ikke lave symlænke %s: %s\n"
