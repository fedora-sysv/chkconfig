msgstr "gebruik:   %s <enable|disable|is-enabled> [naam] \n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "gebruik:   %s [--list] [--type <type>] [naam]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <naam>\n"

#: ../chkconfig.c:55
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <naam>\n"

