#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [name]\n"
msgstr ""

#: ../chkconfig.c:49
#, c-format
msgid "usage:   %s <enable|disable|is-enabled> [name] \n"
msgstr ""

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "upotreba: %s [--list] [--type <vrsta>] [ime]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "          %s --add <ime>\n"

#: ../chkconfig.c:55
#, c-format
msgid "         %s --del <name>\n"
msgstr "          %s --del <ime>\n"

#: ../chkconfig.c:56
#, c-format
msgid "         %s --override <name>\n"
msgstr "          %s --override <ime>\n"

#: ../chkconfig.c:58
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "          %s [--level <nivoi>] [--type <vrsta>] <ime> %s\n"

#: ../chkconfig.c:66
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by "
"native\n"
"      systemd configuration.\n"
"\n"
msgstr ""

#: ../chkconfig.c:73
#, c-format
msgid ""
"      If you want to list systemd services use 'systemctl list-unit-files'.\n"
"      To see services enabled on particular target use\n"
"      'systemctl list-dependencies [target]'.\n"
"\n"
msgstr ""

#: ../chkconfig.c:83
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"servis %s podržava chkconfig, ali nije poznat ni u jednom nivou izvršavanja "
"(izvršite „chkconfig --add %s“)\n"

#: ../chkconfig.c:87
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "servis %s ne podržava chkconfig\n"

#: ../chkconfig.c:89
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "greška pri čitanju podataka o servisu %s: %s\n"

#: ../chkconfig.c:98
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Nemate dovoljne povlastice za izvršavanje ove operacije.\n"

#: ../chkconfig.c:457 ../chkconfig.c:462 ../chkconfig.c:587
msgid "on"
msgstr "uključeno"

#: ../chkconfig.c:457 ../chkconfig.c:462 ../chkconfig.c:587
msgid "off"
msgstr "isključeno"

#: ../chkconfig.c:547
#, c-format
msgid "xinetd based services:\n"
msgstr "servisi zasnovani na xinetd-u:\n"

#: ../chkconfig.c:549
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "neuspešno otvaranje direktorijuma %s: %s\n"

#: ../chkconfig.c:681
#, c-format
msgid "Note: Forwarding request to 'systemctl %s %s'.\n"
msgstr ""

#: ../chkconfig.c:687
#, c-format
msgid "Failed to forward service request to systemctl: %m\n"
msgstr ""

#: ../chkconfig.c:743
#, c-format
msgid "%s version %s\n"
msgstr "%s verzija %s\n"

#: ../chkconfig.c:756
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "--type mora biti „sysv“ ili „xinetd“\n"

#: ../chkconfig.c:762
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr "možete navesti samo jedan od --list, --add, --del ili --override\n"

#: ../chkconfig.c:873
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "samo jedan nivo izvršavanja sme biti naveden u chkconfig upitu\n"

#: ../ntsysv.c:49
msgid "Press <F1> for more information on a service."
msgstr "Pritisnite <F1> za više podataka o servisu."

#: ../ntsysv.c:99 ../ntsysv.c:138
msgid "Ok"
msgstr "U redu"

#: ../ntsysv.c:99
msgid "Back"
msgstr "Nazad"

#: ../ntsysv.c:99
msgid "Cancel"
msgstr "Otkaži"

#: ../ntsysv.c:111
msgid "What services should be automatically started?"
msgstr "Koji servisi treba samostalno da se pokreću?"

#: ../ntsysv.c:118
msgid "Services"
msgstr "Servisi"

#: ../ntsysv.c:333 ../ntsysv.c:362 ../ntsysv.c:400
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "greška pri čitanju podataka o servisu %s: %s\n"

#: ../ntsysv.c:409
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "greška pri čitanju iz direktorijuma %s: %s\n"

#: ../ntsysv.c:446
#, c-format
msgid "You must be root to run %s.\n"
msgstr "Morate biti root da bi izvršili %s.\n"

#: ../ntsysv.c:463
#, c-format
msgid "bad argument to --levels\n"
msgstr "loš argument za --levels\n"

#: ../ntsysv.c:471
#, c-format
msgid "No services may be managed by ntsysv!\n"
msgstr "Nijednim servisom ne sme da upravlja ntsysv!\n"

