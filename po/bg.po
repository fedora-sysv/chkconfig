#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "usage: %s [--list] [--type <тип>] [име]\n"

#: ../chkconfig.c:50
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <име>\n"

