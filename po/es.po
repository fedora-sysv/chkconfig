msgstr "uso:   %s <enable|disable|is-enabled> [nombre] \n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "uso:   %s [--list] [--type <tipo>] [nombre]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nombre>\n"

