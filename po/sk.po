#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "použitie:   %s [--list] [--type <typ>] [názov]\n"

#: ../chkconfig.c:54
#, c-format
