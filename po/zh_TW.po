msgstr "用法：  %s <enable|disable|is-enabled> [name] \n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "用法：%s [--list] [--type <type>] [name]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <name>\n"

