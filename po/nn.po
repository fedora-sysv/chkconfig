#: ../chkconfig.c:49
#, c-format
msgid "usage:   %s <enable|disable|is-enabled> [name] \n"
msgstr "bruk:   %s <enable|disable|is-enabled> [namn] \n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "bruk:   %s [--list] [--type <type>] [namn]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <namn>\n"

#: ../chkconfig.c:55
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <namn>\n"

#: ../chkconfig.c:56
#, c-format
