msgstr "användning: %s <enable|disable|is-enabled> [namn]\n"

#: ../chkconfig.c:52
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "användning:   %s [--list] [--type <typ>] [namn]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <namn>\n"

#: ../chkconfig.c:55
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <namn>\n"

