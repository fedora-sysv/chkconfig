#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "használat:   %s [--list] [--type <típus>] [név]\n"

#: ../chkconfig.c:54
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <név>\n"

#: ../chkconfig.c:55
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <név>\n"

